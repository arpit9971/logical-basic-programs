    "def count_digit(n,d):\n",
    "    count=0\n",
    "    while(n>0):\n",
    "        r=n%10\n",
    "        if r==d:\n",
    "            count=count+1\n",
    "        n=int(n/10)\n",
    "    return count\n",
    "n=int(input(\"enter the number \"))\n",
    "d=int(input(\"enter the digit you want the frequency of:\"))\n",
    "ans=count_digit(n,d)\n",
    "print(ans)"
   ]
