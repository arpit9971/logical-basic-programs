    "def rev(n):\n",
    "    s=0\n",
    "    duplicate=n\n",
    "    while n>0:\n",
    "        r=n%10\n",
    "        s=s*10+r\n",
    "        n=n//10\n",
    "    if s==duplicate:\n",
    "        print(\"pallindrome number:\",s)\n",
    "    else:\n",
    "        add(duplicate,s)\n",
    "def add(n,s):\n",
    "    res=n+s\n",
    "    pallindrome(res)\n",
    "num=int(input(\"Enter the number:\"))\n",
    "rev(num)"
   ]
